{
    "name": "cactus-galaxy/galaxy-store",
    "type": "project",
    "description": "GalaxyStore - demo shop project",
    "keywords": [
        "laravel",
        "framework",
        "shop",
        "translations"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "astrotomic/laravel-translatable": "^11.12",
        "cactus-galaxy/filament-astrotomic": "^0.1.0",
        "davidhsianturi/blade-bootstrap-icons": "^1.4",
        "filament/filament": "^3.1.4",
        "filament/spatie-laravel-media-library-plugin": "^3.1.36",
        "guzzlehttp/guzzle": "^7.8",
        "laravel/breeze": "^1.26.2",
        "laravel/framework": "^10.34.2",
        "laravel/sanctum": "^3.3.2",
        "laravel/tinker": "^2.8.2",
        "spatie/eloquent-sortable": "^4.0",
        "spatie/laravel-medialibrary": "^11.0.4"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23.0",
        "laravel-lang/common": "^5.3",
        "laravel/pint": "^1.13.6",
        "laravel/sail": "^1.26.2",
        "mockery/mockery": "^1.6.6",
        "nunomaduro/collision": "^7.10",
        "pestphp/pest": "^2.26",
        "pestphp/pest-plugin-laravel": "^2.2",
        "spatie/laravel-ignition": "^2.3.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
